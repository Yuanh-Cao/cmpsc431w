{
  "name": "back-end",
  "version": "0.0.0",
  "scripts": {
    "build": "node ./util/build.js",
    "lint": "tslint --project \"tsconfig.json\"",
    "start": "node -r module-alias/register ./dist",
    "tttttttttttttttt": "npm run start:dev;FUNCTION=other npm run test:function",
    "start:dev": "nodemon --config nodemon.json",
    "test:all": "mocha -r ts-node/register src/test/*.ts",
    "test:function": "mocha -r ts-node/register src/test/$FUNCTION.test.ts",
    "reset:db": "rm testdb.sqlite",
    "reset:proddb": "rm prod.sqlite",
    "import": "rm prod.sqlite && ts-node import_departments_script.ts && ts-node import_studentdata_script.ts && ts-node import_profdata_script.ts && ts-node import_postdata_script.ts && ts-node import_coursework_script.ts"
  },
  "_moduleAliases": {
    "@daos": "dist/daos",
    "@entities": "dist/entities",
    "@shared": "dist/shared",
    "@server": "dist/Server"
  },
  "dependencies": {
    "chai": "^4.2.0",
    "command-line-args": "^5.1.1",
    "cookie-parser": "^1.4.5",
    "csvtojson": "^2.0.10",
    "dotenv": "^8.2.0",
    "express": "^4.17.3",
    "express-async-errors": "^3.1.1",
    "helmet": "^3.22.0",
    "http-status-codes": "^1.4.0",
    "mocha": "^7.1.1",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "pug": "^2.0.4",
    "sqlite3": "^4.1.1",
    "typeorm": "^0.2.24",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/command-line-args": "^5.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.6",
    "@types/find": "^0.2.1",
    "@types/helmet": "0.0.45",
    "@types/jsonfile": "^5.0.0",
    "@types/mocha": "^7.0.2",
    "@types/morgan": "^1.9.0",
    "@types/node": "^13.11.1",
    "@types/pug": "^2.0.4",
    "@types/supertest": "^2.0.8",
    "find": "^0.3.0",
    "fs-extra": "^9.0.0",
    "jsonfile": "^6.0.1",
    "nodemon": "^2.0.3",
    "supertest": "^4.0.2",
    "ts-node": "^8.8.2",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.1",
    "typescript": "^3.8.3"
  }
}
